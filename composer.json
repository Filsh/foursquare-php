    "keywords": ["foursquare"],
    "homepage": "http://thetwelvelabs.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Woodford",
            "email": "chris.woodford@gmail.com",
            "homepage": "http://ideapatterns.com"
        }
